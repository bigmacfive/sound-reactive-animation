{
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node",
        "config": {
          "buildCommand": "npm run build",
          "outputDirectory": "build"
        }
      }
    ],
    "routes": [{ "src": "/(.*)", "dest": "/index.html" }],
    "env": {
      "NODE_OPTIONS": "--openssl-legacy-provider"
    }
  }